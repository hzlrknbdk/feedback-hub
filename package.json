{
  "name": "feedbackhub",
  "version": "1.0.0",
  "description": "Form, Notification, Popup & NPS/Survey Builder SaaS Platform",
  "homepage": "https://github.com/hzlrknbdk/feedback-hub",
  "repository": {
    "type": "git",
    "url": "https://github.com/hzlrknbdk/feedback-hub.git"
  },
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
